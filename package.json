{
  "name": "piyushbhai-mehta-personal-website",
  "private": true,
  "description": "Piyushbhai Mehta personal website",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "build": "gatsby build",
    "format": "app-scripts format",
    "start": "gatsby develop -H 0.0.0.0",
    "serve": "gatsby serve -H 0.0.0.0",
    "lint": "app-scripts lint",
    "postinstall": "netlify-lambda install",
    "cy:open": "cypress open",
    "cy:run:record": "cypress run --record --key $(grep CYPRESS_RECORD_KEY .env | cut -d '=' -f2)",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test 'GATSBY_GTAG_DEBUG=true pnpm start' http://localhost:8000 'pnpm cy:open'",
    "test:e2e:ci": "start-server-and-test 'GATSBY_GTAG_DEBUG=true pnpm start' http://localhost:8000 'pnpm cy:run'"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "gatsby": "^5.14.6",
    "gatsby-plugin-emotion": "^8.14.0",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-offline": "^6.14.0",
    "gatsby-plugin-react-helmet": "^6.14.0",
    "netlify-lambda": "^2.0.16",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.3",
    "@viralganatra/app-scripts": "^5.0.3",
    "cypress": "^14.5.4",
    "cypress-axe": "^1.6.0",
    "dotenv-flow": "^4.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^5.1.1",
    "netlify-plugin-cypress": "^2.2.1",
    "start-server-and-test": "^2.0.13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "app-scripts precommit"
    }
  }
}
