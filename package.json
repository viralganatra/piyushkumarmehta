{
  "name": "piyushbhai-mehta-personal-website",
  "private": true,
  "description": "Piyushbhai Mehta personal website",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "develop": "gatsby develop --host 0.0.0.0",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint",
    "format": "biome format --write",
    "cy:open": "cypress open",
    "cy:run:record": "cypress run --record --key $(grep CYPRESS_RECORD_KEY .env | cut -d '=' -f2)",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test 'GATSBY_GTAG_DEBUG=true yarn start' http://localhost:8000 'yarn cy:open'",
    "test:e2e:ci": "start-server-and-test 'GATSBY_GTAG_DEBUG=true yarn start' http://localhost:8000 'yarn cy:run'",
    "netlify": "netlify"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "gatsby": "^5.14.6",
    "gatsby-plugin-emotion": "^8.14.0",
    "gatsby-plugin-google-gtag": "^5.14.0",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-plugin-sitemap": "^6.14.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@testing-library/cypress": "^10.0.3",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "axe-core": "^4.10.3",
    "cypress": "^14.5.4",
    "cypress-axe": "^1.6.0",
    "netlify-cli": "^23.3.0",
    "netlify-plugin-cypress": "^2.2.1",
    "typescript": "^5.3.3"
  }
}
